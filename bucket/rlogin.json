{
  "version": "2.26.1",
  "description": "Rlogin, SSH Terminal",
  "homepage": "https://kmiya-culti.github.io/RLogin/",
  "license": "MIT",
  "architecture": {
    "64bit": {
      "url": "https://github.com/kmiya-culti/RLogin/files/6430626/rlogin_x64.zip",
      "hash": ""
    },
    "32bit": {
      "url": "https://github.com/kmiya-culti/RLogin/files/6430623/rlogin_x32.zip",
      "hash": ""
    }
  },
  "bin": "RLogin.exe",
  "pre_install": [
    "if(!(Test-Path(\"$persist_dir\"))) { New-Item \"$persist_dir\" -type directory | Out-Null }",
    "if(!(Test-Path(\"$persist_dir\\RLogin.ini\"))) { New-Item \"$persist_dir\\RLogin.ini\" -type file | Out-Null }"
  ],
  "uninstaller": {
    "script": "if (Test-Path \"$dir\\RLogin.ini\") { Copy-Item \"$dir\\RLogin.ini\" \"$persist_dir\" -Force }"
  },
  "persist": [
    "RLogin.ini"
  ],
  "shortcuts": [
    ["RLogin.exe", "RLogin"]
  ],
  "checkver": {
    "github": "https://github.com/kmiya-culti/RLogin/"
  }
}
